class Solution {
    public int removeElement(int[] nums, int val) {
        int j=0,n=nums.length,non_val=0;

        for(int i=0;i<n;i++)
        {
            if(nums[i]!=val){
                nums[j]=nums[i];
                non_val++;
                j++;
            }
        }
        return non_val;
    }
}
